/// Daily charts group hub.
module server.pgs.daily;

cgi = load es.dm.cgi;
load std;
daily = load server.pgs.daily.daily;

{
  /// Request process.
  ///
  /// Arguments:
  ///   ck - Comunication key.
  ///   rq - Request.
process ck rq =
    case cgi.rqString rq "source" of
    "daily" : daily.process ck rq;
    k : failedKey k
    esac;
}