/// Main home page.
module server.pgs.home;

cgi = load es.dm.cgi;
load std;
hom = load server.pgs.home.home;

{
  /// Request process.
  ///
  /// Arguments:
  ///   ck - Comunication key.
  ///   rq - Request.
process ck rq =
    case cgi.rqString rq "source" of
    "home" : hom.process ck rq;
    k : failedKey k
    esac;
}