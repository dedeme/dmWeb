module changePass;

load es.dm.all;
json = load es.dm.json;

{
  process ck rq is
  string -> map<string, json.t> -> string =
    cgi.changePass ck (json.rs rq["user"]) (json.rs rq["old"])
                      (json.rs rq["new"]),
}
