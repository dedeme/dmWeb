module cts;

{
  /// `[appName]` is "YetiDoc".
  appName = "YetiDoc",

  /// `[dataVersion]` is "202004".
  dataVersion = "202004",

  /// `[dataDir]` is "dmcgi/YetiDoc".
  appDir = "dmcgi/YetiDoc",

  /// `[appExpiration]` is 1800 -> 1/2 hour.
  appExpiration = 1800,

  /// `[version ()]` returns the text of file "version.txt".
  versionText () = "\(appName)\nData version: \(dataVersion)"
}