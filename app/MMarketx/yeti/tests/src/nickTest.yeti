module nickTest;

load es.dm.assert;
nick = load data.nick;

{
  run () =
    println "Nick test:";
    nk = nick.mk 23 "TEF";
    eq (nick.toJs nk) (nick.toJs nk |> nick.fromJs |> nick.toJs);
    yes (not nk.sel?);
    nk = nk with { sel? = true };
    eq (nick.toJs nk) (nick.toJs nk |> nick.fromJs |> nick.toJs);
    yes nk.sel?;
    println "  Finished",
}
