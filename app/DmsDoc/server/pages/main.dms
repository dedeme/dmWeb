"../dm/cgi" import
"../io" import

(@+m:: Rq =
  Rq .rq js,rs : rq =

  ( "getDb" rq ==)
  (
    {
      "conf": io,readConf
      "libs": io,readLibs
    } map,from : cgi,ok
  )
  else
  ( "setConf" rq ==)
  (
    Rq .conf io,writeConf
    cgi,none
  )
  else
  ( "Value '" rq "' not allowed for 'rq'" + + fail)
  if
@-s) process =>
// OBJ<Js>
