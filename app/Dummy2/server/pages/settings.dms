"../dm/cgi" import
"../conf" import

( @m :: Rq =
  Rq "rq" map,has? ! ("Field 'rq' is missing" fail) if
  Rq .rq js,rs : rq =

  ( "lang" rq ==)
  ( conf,lang "es" == ("en") ("es") elif : conf,setLang; cgi,none)
  else
  ( "chpass" rq ==)
  ( Rq .user js,rs : Rq .pass js,rs : Rq .newPass js,rs : cgi,changePass)
  else
  ( "Value '" rq "' not allowed for 'rq'" + + fail)
  if
