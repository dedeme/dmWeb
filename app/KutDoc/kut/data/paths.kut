/// 'ppath' array serialization.

import "data/ppath";

/// Serializes an array of mpaths
/// [m...] -> s
toJs = \Ps -> return js.wa(arr.mp(Ps, \P -> return ppath.toJs(P);));;

/// Restore an array of mpaths
/// s -> [m...]
fromJs = \jj -> return arr.mp(js.ra(jj), \j -> return ppath.fromJs(j););;


