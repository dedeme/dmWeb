/// Configuration data.

/// \s, s, b -> m
new = \path, lang, showAll -> return {
    "path": path,
    "lang": lang,
    "showAll": showAll
  };
;

/// \m -> s
toJs = \Cf -> return js.wa([
    js.ws(Cf.path),   
    js.ws(Cf.lang),   
    js.wb(Cf.showAll)
  ]);
;  

/// \s -> m
fromJs = \j -> {
  A = js.ra(j);
  return new(
    js.rs(A[0]),
    js.rs(A[1]),
    js.rb(A[2])
  );
};

