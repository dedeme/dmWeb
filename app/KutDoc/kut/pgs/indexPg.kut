/// Package index page.

import "dm/cgi";
import "readers/ixReader";
import "data/ixTree";

/// Process request.
/// \m -> s
process = \Rq -> {
  rq = js.rs(Rq.rq);
  switch (rq) {
    "index": {
      pack = js.rs(Rq.pack);
      IxT = ixReader.read(pack);      
      return cgi.rp({
        "index": arr.size(IxT) == 0
          ? js.wn()
          : ixTree.toJs(IxT[0])
      });
    }
    default: sys.fail("Value of rq (" + rq + ") is not valid");
  };
};


